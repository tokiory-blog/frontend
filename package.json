{
  "name": "station",
  "private": true,
  "scripts": {
    "build": "pnpm nuxt build",
    "generate": "nuxt generate",
    "app:dev": "nuxt dev",
    "app:prod": "NODE_ENV=\\\"production\\\" && node .output/server/index.mjs",
    "app:preview": "nuxt preview",
    "process:eslint": "eslint --ext .js,.ts,.vue --fix",
    "process:prettier": "prettier",
    "vercel:dev": "vercel",
    "vercel:prod": "pnpm build && pnpm vercel --prod",
    "env:prod": "shx ln -sf env/prod.env .env",
    "env:dev": "shx ln -sf env/dev.env .env",
    "test:unit": "vitest run --config ./vitest.config.ts --silent",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:record": "playwright codegen",
    "test:all": "concurrently -n vitest,playwright 'npm:test:unit' 'npm:test:e2e'",
    "postinstall": "pnpm generate",
    "prepare": "concurrently -n husky,env 'husky install' 'pnpm env:prod'"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@nuxt/content": "^2.5.2",
    "@nuxt/test-utils": "^3.4.0",
    "@playwright/test": "^1.35.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "dotenv": "^16.1.4",
    "emoji-type": "^1.0.4",
    "eslint": "^8.37.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "got": "^12.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "nuxt": "^3.3.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "sass": "^1.60.0",
    "shx": "^0.3.4",
    "typedoc": "^0.24.1",
    "typedoc-plugin-markdown": "^3.15.1",
    "typescript": "^5.0.4",
    "vercel": "^28.18.5",
    "vitest": "^0.30.1",
    "vue": "^3.2.47"
  },
  "dependencies": {
    "@resvg/resvg-js": "^2.4.1",
    "h3": "^1.6.4",
    "h3-zod": "^0.4.2",
    "normalize.css": "^8.0.1",
    "nuxt-content-assets": "^1.3.2",
    "perfect-debounce": "^0.1.3",
    "satori": "^0.4.7",
    "satori-html": "^0.3.2",
    "shiki-es": "^0.2.0",
    "zod": "^3.21.4"
  },
  "browserslist": [
    "defaults",
    "not dead",
    "last 3 version",
    "Chrome >= 35",
    "Firefox >= 38",
    "ie >= 10",
    "iOS >= 8",
    "Safari >= 12"
  ]
}
