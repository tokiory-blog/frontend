---
import UILogo from './UILogo.vue';
---

<header class="header">
  <div class="header__logo">
    <UILogo />
  </div>
  <nav class="header__navigation navigation">
    <ul class="navigation__list">
      <li class="navigation__item">
        <a class="navigation__link"></a>
      </li>
    </ul>
  </nav>
</header>

<style lang="scss">
  .header {
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  
  .navigation {
    
    &__list {
     margin: 0;
     display: flex;
     align-items: center;
    }
  }
</style>